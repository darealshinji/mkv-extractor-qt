#!/usr/bin/make -f

%:
	dh ${@} --parallel --with python3

override_dh_auto_clean:
	dh_auto_clean
	rm -f mkv-extractor-qt5.svgz

override_dh_auto_build:
	dh_auto_build
	gzip -c -f9 mkv-extractor-qt5.svg > mkv-extractor-qt5.svgz

override_dh_python3:
	dh_python3 --ignore-shebangs

override_dh_pysupport:

